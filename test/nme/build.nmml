<?xml version="1.0" encoding="utf-8"?>
<project>
  
  <meta
     title="Video Application"
     package="com.nmehost.numerix"
     version="1.0.0"
     company="numerix" 
  />
  
  <app
     file="Application" 
     main="Main"
  />
  
  <window
     width="640" 
     height="480" 
     orientation="landscape" 
     fps="60"
     background="0xeeeeee"
     require_shaders="true"
     resizable="true"
     hardware="true"
  />

  <source path="src" />
  <haxelib name="nme" />
  <ndll name="numerix" haxelib="numerix" />
</project>
